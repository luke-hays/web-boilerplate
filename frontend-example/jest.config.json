{
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 80,
      "lines": 80,
      "statements": 80
    }
  },
  "setupFilesAfterEnv": [
    "<rootDir>/src/setupTests.ts"
  ],
  "coveragePathIgnorePatterns": [
    "node_modules",
    "<rootDir>/src/index.tsx"
  ],
  "testEnvironment": "jsdom"
}